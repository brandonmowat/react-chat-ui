{
  "name": "react-chat-ui",
  "version": "0.2.2",
  "description": "A library of React components for building chat UI's",
  "repository": "brandonmowat/react-chat-ui",
  "main": "./lib/index.js",
  "scripts": {
    "build-demo":
      "babel --presets es2015,stage-0 ./demo/index.js -o ./demo/es5.js && browserify demo/es5.js -o demo/bundle.js",
    "compile": "babel --presets es2015,stage-0,react -d lib/ temp/",
    "prepublish": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp watch"
  },
  "keywords": ["react", "chat", "chatfeed", "reactjs", "ui", "messenger"],
  "author": "Brandon Mowat",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "browserify": "^14.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^3.2.2",
    "typescript": "^2.5.2"
  }
}
